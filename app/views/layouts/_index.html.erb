<% provide(:title, "#{type}s") %>
<% provide(:heading, "#{type} Dashboard") %>

<div class="container-fluid">
    <div class="row">
          <div class="col-md-10 col-sm-10 col-xs-10 pull-left">
            <%= render partial: 'layouts/index/search' %>
          </div>
          <div class="col-md-2 col-sm-2 col-xs-2 pull-left">
            <%= render partial: 'layouts/index/add_button', locals: {name: type} %>
          </div>
    </div>
    <p>
    <div class="row">
      <table class="table table-striped table-hover">
        <thead><%= render partial: 'layouts/index/thead', locals: {titles: @column_titles} %></thead>
        <tbody><%= render partial: 'layouts/index/tbody', locals: {items: items} %></tbody>
      </table>
    </div>
</div>
